<!DOCTYPE html>
<html lang="en">
  <head>
    {{>head}}
  </head>
  <body>
    
	<nav class="navbar navbar-expand-lg navbar-dark ftco_navbar bg-dark ftco-navbar-light" id="ftco-navbar">
	    <div class="container">
	      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#ftco-nav" aria-controls="ftco-nav" aria-expanded="false" aria-label="Toggle navigation">
	        <span class="oi oi-menu"></span> Menu
	      </button>

	      <div class="collapse navbar-collapse" id="ftco-nav">
            <p style ="color:white;"> Angel Quinteros <p>
	        <ul class="navbar-nav ml-auto">
	        	<li class="nav-item"><a href="/" class="nav-link">Home</a></li>
	        	<li class="nav-item"><a href="/products" class="nav-link">Products</a></li>
	        	<li class="nav-item"><a href="/signup" class="nav-link">Sign In</a></li>
				<li class="nav-item"><a href="#aboutAngel" class="nav-link">About</a></li>
				<li class="nav-item"><a href="" class="nav-link"><i class="fas fa-shopping-cart"></i>
					 Shopping Cart</a></li>
	        </ul>
	      </div>
	    </div>
	  </nav>
    <!-- END nav -->
    
    <section class="home-slider js-fullheight owl-carousel bg-black">

      <div class="slider-item js-fullheight">
      	<div class="overlay"></div>
        <div class="container-fluid p-0">
          <div class="row d-md-flex no-gutters slider-text js-fullheight align-items-center justify-content-end" data-scrollax-parent="true">
          	<div class="one-third order-md-last img js-fullheight" style="background-image:url(images/graffiti.png);">
          		<div class="overlay"></div>
          	</div>
	          <div class="one-forth d-flex js-fullheight align-items-center ftco-animate" data-scrollax=" properties: { translateY: '100%' }">
	          	<div class="text mt-md-5">
		           
	            </div>
	          </div>
        	</div>
        </div>
      </div>

      <div class="slider-item js-fullheight">
      	<div class="overlay"></div>
        <div class="container-fluid p-0">
          <div class="row d-flex no-gutters slider-text js-fullheight align-items-center justify-content-end" data-scrollax-parent="true">
          	<div class="one-third order-md-last img js-fullheight" style="background-image:url(images/kod.png);">
          		<div class="overlay"></div>
          	</div>
	          <div class="one-forth d-flex js-fullheight align-items-center ftco-animate" data-scrollax=" properties: { translateY: '70%' }">
	          	<div class="text mt-md-5">
		            
	            </div>
	          </div>
        	</div>
        </div>
      </div>
      
    </section>
	

		<section class="ftco-section ftco-wrap-about ftco-no-pb ftco-no-pt bg-black">
			<div class="container">
				<div class="row no-gutters">
					<div class="col-sm-5 img img-2 d-flex " id="aboutAngel" style="background-image: url(images/about.png); position: relative; opacity:0.4;"></div>
					<div class="col-sm-7 wrap-about py-5 ftco-animate">
	          <div class="heading-section mt-5 mb-4">
	          	<div class="pl-lg-5 ml-md-5">
		          	<h1 style="color:white">About the artist</h1>
	            </div>
	          </div>
	          <div class="pl-lg-5 ml-md-5">
							<p>{{artist}}</p>
							<div class="thumb my-4 d-flex">
	            </div>
						</div>
					</div>
				</div>
			</div>
		</section>
			

	

        {{>footer}}
  

  <!-- loader -->
  <div id="ftco-loader" class="show fullscreen"><svg class="circular" width="48px" height="48px"><circle class="path-bg" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke="#eeeeee"/><circle class="path" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke-miterlimit="10" stroke="#F96D00"/></svg></div>


  {{>scripts}}
    
  </body>
</html>